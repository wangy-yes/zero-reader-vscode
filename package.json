{
  "name": "zero-reader",
  "displayName": "zero-reader",
  "description": "read in console.",
  "version": "0.0.1",
  "publisher": "Wangyes",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:ZeroSidebar"
  ],
  "main": "./src/index.js",
  "contributes": {
    "commands": [
      {
        "command": "zero-reader.selectFile",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "zero_sidebar",
          "title": "zero-reader",
          "icon": "images/zero.svg"
        }
      ]
    },
    "views": {
      "zero_sidebar": [
        {
          "id": "chapters",
          "name": "目录"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "chapters",
        "contents": "没有发现文件， [请先选择文件](https://www.npmjs.com/).\n[选择文件](command:zero-reader.selectFile)"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}